<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScFaqType" table="SC_FAQ_TYPE">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="typeName" type="java.lang.String">
            <column name="TYPE_NAME" length="100">
                <comment>分类名称</comment>
            </column>
        </property>
        <property name="typeDesc" type="java.lang.String">
            <column name="TYPE_DESC" length="100">
                <comment>分类描述</comment>
            </column>
        </property>
        <property name="typeCode" type="java.lang.String">
            <column name="TYPE_CODE" length="10">
                <comment>分类代码</comment>
            </column>
        </property>
        <property name="order" type="java.lang.Integer">
            <column name="ORDER_">
                <comment>分类排序</comment>
            </column>
        </property>
        <property name="level" type="java.lang.String">
            <column name="LEVEL_" length="1">
                <comment>所属级别</comment>
            </column>
        </property>
        <property name="parentTypeId" type="java.lang.String">
            <column name="PARENT_TYPE_ID" length="36">
                <comment>上级类别ID</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="flag" type="java.lang.String">
            <column name="FLAG" length="1">
                <comment>数据有效性</comment>
            </column>
        </property>
        <property name="belong" type="java.lang.String">
            <column name="BELONG" length="36">
                <comment>所属</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
