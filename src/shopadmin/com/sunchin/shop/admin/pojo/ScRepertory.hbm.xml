<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScRepertory" table="SC_REPERTORY">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned" />
        </id>
        <property name="goodsId" type="java.lang.String">
            <column name="GOODS_ID" length="36" />
        </property>
        <property name="childGoodsId" type="java.lang.String">
            <column name="CHILD_GOODS_ID" length="36" />
        </property>
        <property name="availableNum" type="java.lang.Integer">
            <column name="AVAILABLE_NUM" />
        </property>
        <property name="salesNum" type="java.lang.Integer">
            <column name="SALES_NUM" />
        </property>
        <property name="freezeNum" type="java.lang.Integer">
            <column name="FREEZE_NUM" />
        </property>
        <property name="badNum" type="java.lang.Integer">
            <column name="BAD_NUM" />
        </property>
        <property name="salesCount" type="java.lang.Integer">
            <column name="SALES_COUNT" />
        </property>
        <property name="purchaseCount" type="java.lang.Integer">
            <column name="PURCHASE_COUNT" />
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7" />
        </property>
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID" length="36" />
        </property>
        <property name="flag" type="java.lang.String">
            <column name="FLAG" length="1" />
        </property>
        <property name="belong" type="java.lang.String">
            <column name="BELONG" length="10" />
        </property>
    </class>
</hibernate-mapping>
