<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScBrand" table="SC_BRAND">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="brandName" type="java.lang.String">
            <column name="BRAND_NAME" length="36">
                <comment>品牌名</comment>
            </column>
        </property>
        <property name="brandDesc" type="java.lang.String">
            <column name="BRAND_DESC">
                <comment>品牌简介</comment>
            </column>
        </property>
        <property name="brandLogo" type="java.lang.String">
            <column name="BRAND_LOGO" length="100">
                <comment>品牌logo</comment>
            </column>
        </property>
        <property name="flag" type="java.lang.String">
            <column name="FLAG" length="1">
                <comment>状态</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="createPeople" type="java.lang.String">
            <column name="CREATE_PEOPLE" length="36">
                <comment>创建人</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
