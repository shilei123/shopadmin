<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScUserGrade" table="SC_USER_GRADE" >
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
         <property name="userId" type="java.lang.String">
             <column name="USER_ID" length="36">
                <comment>会员id</comment>
            </column>
        </property>
         <property name="gradeId" type="java.lang.String">
             <column name="GRADE_ID" length="36">
                <comment>等级id</comment>
            </column>
        </property>
          <property name="createTime" type="java.util.Date">
               <column name="CREATE_TIME">
                <comment>创建时间</comment>
            </column>
        </property>
         <property name="optionTime" type="java.util.Date">
               <column name="OPTION_TIME">
                <comment>修改时间</comment>
            </column>
        </property>
         <property name="flag" type="java.lang.String">
               <column name="FLAG" length="1">
                <comment>数据有效性/默认0，0有效，1无效</comment>
            </column>
        </property>
         <property name="belong" type="java.lang.String">
               <column name="BELONG" length="50">
                <comment>所属</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
