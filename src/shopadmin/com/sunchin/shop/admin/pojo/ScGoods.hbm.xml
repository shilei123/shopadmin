<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScGoods" table="SC_GOODS">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="brandId" type="java.lang.String">
            <column name="BRAND_ID" length="36" />
        </property>
        <property name="title" type="java.lang.String">
            <column name="TITLE" length="100" />
        </property>
        <property name="subTitle" type="java.lang.String">
            <column name="SUB_TITLE" length="100" />
        </property>
        <property name="goodsName" type="java.lang.String">
            <column name="GOODS_NAME" length="100" />
        </property>
        <property name="detail" type="java.lang.String">
            <column name="DETAIL" />
        </property>
        <property name="params" type="java.lang.String">
            <column name="PARAMS" />
        </property>
        <property name="stopTime" type="java.util.Date">
            <column name="STOP_TIME" length="7" />
        </property>
        <property name="goodsCode" type="java.lang.String">
            <column name="GOODS_CODE" length="10" />
        </property>
        <property name="isuse" type="java.lang.String">
            <column name="ISUSE" length="1" />
        </property>
        <property name="emptyStore" type="java.lang.String">
            <column name="EMPTY_STORE" length="1" />
        </property>
        <property name="putaway" type="java.lang.String">
            <column name="PUTAWAY" length="1" />
        </property>
        <property name="virtual" type="java.lang.String">
            <column name="VIRTUAL" length="1" />
        </property>
        <property name="purchasePrice" type="java.lang.Double">
            <column name="PURCHASE_PRICE" precision="10" />
        </property>
        <property name="marketPrice" type="java.lang.Double">
            <column name="MARKET_PRICE" precision="10" />
        </property>
        <property name="salePrice" type="java.lang.Double">
            <column name="SALE_PRICE" precision="10" />
        </property>
        <property name="promotionPrice" type="java.lang.Double">
            <column name="PROMOTION_PRICE" precision="10" />
        </property>
        <property name="goodsNo" type="java.lang.String">
            <column name="GOODS_NO" length="10" />
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7" />
        </property>
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID" length="10" />
        </property>
        <property name="flag" type="java.lang.String">
            <column name="FLAG" length="1" />
        </property>
        <property name="belong" type="java.lang.String">
            <column name="BELONG" length="10" />
        </property>
    </class>
</hibernate-mapping>