<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScPropValue" table="SC_PROPVALUE">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="valCode" type="java.lang.String">
            <column name="VAL_CODE" length="36">
                <comment>属性值code</comment>
            </column>
        </property>
        <property name="valName" type="java.lang.String">
            <column name="VAL_NAME" length="36">
                <comment>属性值name</comment>
            </column>
        </property>
        <property name="valOrder" type="java.lang.String">
            <column name="VAL_ORDER" length="2">
                <comment>排序</comment>
            </column>
        </property>
        <property name="flag" type="java.lang.String">
            <column name="FLAG" length="1">
                <comment>状态</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7">
                <comment>创建日期</comment>
            </column>
        </property>
        <property name="createPeople" type="java.lang.String">
            <column name="CREATE_PEOPLE" length="36">
                <comment>创建人</comment>
            </column>
        </property>
        <property name="updateTime" type="java.util.Date">
            <column name="UPDATE_TIME" length="7">
                <comment>修改日期</comment>
            </column>
        </property>
        <property name="updatePeople" type="java.lang.String">
            <column name="UPDATE_PEOPLE" length="36">
                <comment>修改人</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
