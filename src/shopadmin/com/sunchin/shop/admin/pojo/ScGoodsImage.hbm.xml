<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScGoodsImage" table="SC_GOODS_IMAGE">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned" />
        </id>
        <property name="goodsId" type="java.lang.String">
            <column name="GOODS_ID" length="36" />
        </property>
        <property name="childGoodsId" type="java.lang.String">
            <column name="CHILD_GOODS_ID" length="36" />
        </property>
        <property name="imageId" type="java.lang.String">
            <column name="IMAGE_ID" length="100" />
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7" />
        </property>
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID" length="10" />
        </property>
        <property name="flag" type="java.lang.String">
            <column name="FLAG" length="1" />
        </property>
    </class>
</hibernate-mapping>
