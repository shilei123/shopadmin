<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScProperty" table="SC_PROPERTY">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="propCode" type="java.lang.String">
            <column name="PROP_CODE" length="36">
                <comment>属性code</comment>
            </column>
        </property>
        <property name="propName" type="java.lang.String">
            <column name="PROP_NAME" length="36">
                <comment>属性name</comment>
            </column>
        </property>
        <property name="order" type="java.lang.Integer">
            <column name="ORDER_">
                <comment>排序</comment>
            </column>
        </property>
        <property name="flag" type="java.lang.String">
            <column name="FLAG" length="1">
                <comment>状态</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7">
                <comment>创建日期</comment>
            </column>
        </property>
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID" length="36">
                <comment>创建人</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
