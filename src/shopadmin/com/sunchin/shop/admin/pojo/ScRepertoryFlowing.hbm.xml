<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScRepertoryFlowing" table="SC_REPERTORY_FLOWING" >
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="goodsId" type="java.lang.String">
               <column name="GOODS_ID" length="36">
                <comment>商品ID</comment>
            </column>
        </property>
        <property name="childGoodsId" type="java.lang.String">
             <column name="CHILD_GOODS_ID" length="36">
                <comment>商品子表id</comment>
            </column>
        </property>
        <property name="aboutType" type="java.lang.String">
             <column name="ABOUT_TYPE" length="1">
                <comment>关联类型/单据 1 发货单 2</comment>
            </column>
        </property>
        <property name="aboutId" type="java.lang.String">
             <column name="ABOUT_ID" length="36">
                <comment>关联ID</comment>
            </column>
        </property>
        <property name="repertoryType" type="java.lang.String">
             <column name="REPERTORY_TYPE" length="1">
                <comment>库存类型/可用库存 1 销售库存 2 冻结库存 3 报损数量 4</comment>
            </column>
        </property>
       <property name="repertoryNum" type="java.lang.Integer">
             <column name="REPERTORY_NUM">
                <comment>库存数量</comment>
            </column>
        </property>
         <property name="createTime" type="java.util.Date">
               <column name="CREATE_TIME">
                <comment>申请时间</comment>
            </column>
        </property>
         <property name="createUserId" type="java.lang.String">
             <column name="CREATE_USER_ID" length="36">
                <comment>创建人</comment>
            </column>
        </property>
        <property name="flag" type="java.lang.String">
               <column name="FLAG" length="1">
                <comment>数据有效性/默认1，1有效，0无效</comment>
            </column>
        </property>
        <property name="belong" type="java.lang.String">
               <column name="BELONG" length="36">
                <comment>所属</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
